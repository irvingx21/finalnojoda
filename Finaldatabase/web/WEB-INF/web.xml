<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Sesion</servlet-name>
        <servlet-class>Servlet.Sesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registripropietario</servlet-name>
        <servlet-class>Servlet.Registripropietario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registromatricula</servlet-name>
        <servlet-class>Servlet.Registromatricula</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registrovehiculo</servlet-name>
        <servlet-class>Servlet.Registrovehiculo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActualizarMatricula</servlet-name>
        <servlet-class>Servlet.ActualizarMatricula</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Sesion</servlet-name>
        <url-pattern>/iniciar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registripropietario</servlet-name>
        <url-pattern>/registropro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registromatricula</servlet-name>
        <url-pattern>/registromatricula</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registrovehiculo</servlet-name>
        <url-pattern>/registrovehiculo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActualizarMatricula</servlet-name>
        <url-pattern>/actualizamatricula</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
